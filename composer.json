{
  "name": "sportlog/fit",
  "description": "A decoder for FIT files",
  "type": "library",
  "keywords": [
    "fit",
    "garmin"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Johannes Aberidis"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sportlog\\FIT\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sportlog\\FIT\\": "src/",
      "Sportlog\\FIT\\Test\\": "tests/",
      "Sportlog\\FIT\\Generator\\": "config/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.64",
    "phpunit/phpunit": "^9.5",
    "nette/php-generator": "^3.5"
  },
  "require": {
    "php": ">=8.0"
  },
  "scripts": {
    "phpunit": "vendor\\bin\\phpunit --testsuite fit",
    "phpstan": "vendor\\bin\\phpstan analyse -c phpstan.neon"
  }
}
